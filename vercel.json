{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://erichsimon.com/blog/"
    },
    {
      "source": "/test/:path*",
      "destination": "https://erichsimon.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/property-management/(.*)",
      "headers": [
        {
          "key": "x-vercel-enable-rewrite-caching",
          "value": "1"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
